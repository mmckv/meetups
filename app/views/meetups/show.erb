<h1><%= @event.name %></h1>
<ul>
  <li>Creator:
    <%= @event.user.username %></li>
</br>
<li>Location:
  <%= @event.location %></li>
</br>

<li>Description:
<%= @event.description %></li>
</br>
<li>Members:</li>
</br>
<ul style="list-style-type: none;">
<% @event.users.each do |user| %>
<li>
<img src="<%=user.avatar_url%>" height="42" width="42">
<%= user.username %>
</li>
<% end %>
</ul>
</br>
<form action="/meetups/<%= @event.id %>" method="post">
<input type="submit" value="Join Meetup">
</form>
</ul>
</ul>
